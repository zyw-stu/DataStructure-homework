#include <stdio.h>
//第i行最大值，第j列最小值 
#define N 20 
#define M 20
typedef struct{
	int col;
	int val;	
}maxRow[N];
int main()
{
	int a[N][M];
	for(i=0;i<N;i++){
		for(j=0;j<M;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	int i,j;
	for(i=0;i<N;i++){
		maxRow[i].col=j;maxRow[i].val=a[i][0];
		for(j=1;j<M;j++){
			if(a[i][j]>maxRow[i].val)
			maxRow[i].val=a[i][j];	
		}
	}
	int min;
	int flag; int f=0;
	for(i=0;i<N;i++){
		min=maxRow[i].val;
		int col=maxRow[i].col;
		flag=1;
		for(i=0;i<N;i++){
			if(a[i][col]<min){
				flag=0;
				break;
			}
		}
		if(flag){
			f=1;
			printf("鞍点所在的行列分别为：%d,%d\n",i,maxRow[i].col);
		}
	}
	if(f==0) printf("不存在鞍点\n");
	return 0;
}
